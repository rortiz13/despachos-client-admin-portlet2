<?xml version="1.0" encoding="UTF-8"?>
<custom-sql>

	<sql id="la.netco.despachos_judiciales.entities.service.persistence.DistritoFinder.findByDespacho">
	 <![CDATA[
		SELECT DISTINCT {DISTRITO.*} FROM GLO_DISTRITOS DISTRITO
		JOIN GLO_CONSEJOS_SEC CONSEJO ON DISTRITO.DIS_CONSEJO_SEC = CONSEJO.CON_CODIGO 
		JOIN GLO_CIRCUITOS CIRCUITO ON CIRCUITO.CIR_DISTRITO = DISTRITO.DIS_CODIGO
		JOIN GLO_MUNICIPIOS MUNICIPIO ON (MUNICIPIO.MUN_CIRCUITO = CIRCUITO.CIR_CODIGO  OR MUNICIPIO.MUN_CIRCUITO_ADM = CIRCUITO.CIR_CODIGO OR MUNICIPIO.MUN_CIRCUITO_DIS = CIRCUITO.CIR_CODIGO)
		WHERE  CONSEJO.CON_CODIGO =  ? AND MUNICIPIO.MUN_DEPARTAMENTO = ? AND MUNICIPIO.MUN_CODIGO = ? AND 1=1
		ORDER BY DIS_NOMBRE 
	  ]]>
	</sql>
	
	<sql id="la.netco.despachos_judiciales.entities.service.persistence.DistritoFinder.findByFields">
	 <![CDATA[
		SELECT {DISTRITO.*}  FROM GLO_DISTRITOS DISTRITO
		WHERE DISTRITO.DIS_CODIGO = ? AND DISTRITO.DIS_NOMBRE  = ? AND DISTRITO.DIS_CONSEJO_SEC  = ? AND DISTRITO.DIS_JURISDICCION  = ? AND DISTRITO.DIS_CODIGO_DANE  = ? AND 1=1
		ORDER BY  DISTRITO.DIS_CODIGO
	  ]]>
	</sql>
	
	<sql id="la.netco.despachos_judiciales.entities.service.persistence.DistritoFinder.countByFields">
	 <![CDATA[
		SELECT COUNT(DISTRITO.DIS_CODIGO) AS COUNT_VALUE  FROM GLO_DISTRITOS DISTRITO
		WHERE DISTRITO.DIS_CODIGO = ? AND  DISTRITO.DIS_NOMBRE  = ? AND DISTRITO.DIS_CONSEJO_SEC  = ? AND DISTRITO.DIS_JURISDICCION  = ? AND DISTRITO.DIS_CODIGO_DANE  = ? AND 1=1
	  ]]>
	</sql>

	
	
</custom-sql>