<?xml version="1.0" encoding="UTF-8"?>
<custom-sql>

	<sql id="la.netco.despachos_judiciales.entities.service.persistence.MunicipioFinder.findByEstadoDescpacho">
	 <![CDATA[
	 	select DISTINCT {MUNICIPIO.*} from GLO_MUNICIPIOS MUNICIPIO   
		join GLO_DEPARTAMENTOS DEPARTAMENTO on MUNICIPIO.MUN_DEPARTAMENTO = DEPARTAMENTO.DEP_CODIGO  
		join ADM_DESPACHOS  DESPACHO on (MUNICIPIO.MUN_CODIGO = DESPACHO.DES_MUNICIPIO and DEPARTAMENTO.DEP_CODIGO =DESPACHO.DES_DEPARTAMENTO ) 
		where  DEPARTAMENTO.DEP_CODIGO = ? and DESPACHO.DES_ESTADO = 1  order by MUNICIPIO.MUN_NOMBRE asc 
	  ]]>
	</sql>

	<sql id="la.netco.despachos_judiciales.entities.service.persistence.MunicipioFinder.findByCompositeKey">
	 <![CDATA[
	 	SELECT {MUNICIPIO.*}
		FROM GLO_MUNICIPIOS MUNICIPIO 
		WHERE MUNICIPIO.MUN_DEPARTAMENTO = ? AND MUNICIPIO.MUN_CODIGO = ? AND 1=1
	  ]]>
	</sql>
	
	<sql id="la.netco.despachos_judiciales.entities.service.persistence.MunicipioFinder.findByFields">
	 <![CDATA[
	 	SELECT {MUNICIPIO.*} FROM GLO_MUNICIPIOS MUNICIPIO
		WHERE MUNICIPIO.MUN_DEPARTAMENTO = ? AND MUNICIPIO.MUN_CODIGO = ? AND MUNICIPIO.MUN_NOMBRE = ? AND MUNICIPIO.MUN_INDICATIVO = ? AND MUNICIPIO.MUN_CIRCUITO = ? AND MUNICIPIO.MUN_CIRCUITO_ADM = ? AND MUNICIPIO.MUN_CONSEJO_SEC_DIS = ? AND MUNICIPIO.MUN_CIRCUITO_DIS = ? AND 1=1
		ORDER BY MUNICIPIO.MUN_DEPARTAMENTO,MUNICIPIO.MUN_CODIGO 
	  ]]>
	</sql>
	
	<sql id="la.netco.despachos_judiciales.entities.service.persistence.MunicipioFinder.countByFields">
	 <![CDATA[
	 	SELECT COUNT(MUNICIPIO.MUN_CODIGO) AS COUNT_VALUE FROM GLO_MUNICIPIOS MUNICIPIO
		WHERE MUNICIPIO.MUN_DEPARTAMENTO = ? AND MUNICIPIO.MUN_CODIGO = ? AND MUNICIPIO.MUN_NOMBRE = ? AND MUNICIPIO.MUN_INDICATIVO = ? AND MUNICIPIO.MUN_CIRCUITO = ? AND MUNICIPIO.MUN_CIRCUITO_ADM = ? AND MUNICIPIO.MUN_CONSEJO_SEC_DIS = ? AND MUNICIPIO.MUN_CIRCUITO_DIS = ? AND 1=1
	  ]]>
	</sql>
	
	
</custom-sql>